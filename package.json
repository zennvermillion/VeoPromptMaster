{
  "name": "veo-prompt-master",
  "version": "3.0.8",
  "productName": "Veo Prompt Master",
  "description": "A tool to generate cinematic prompts for AI video",
  "author": "Zenn Vermillion",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
  "start": "electron .",
  "build": "electron-builder",
  "release": "electron-builder --publish onTag"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/zennvermillion/VeoPromptMaster.git"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "electron-log": "^5.4.1",
    "electron-store": "8",
    "electron-updater": "^6.6.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.zennvermillion.veopromptmaster",
    "productName": "Veo Prompt Master",
    "publish": [
      {
        "provider": "github",
        "owner": "zennvermillion",
        "repo": "VeoPromptMaster"
      }
    ],
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    }
  }
}
